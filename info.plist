				<key>script</key>
				<string>#!/bin/bash

# Find trimmy binary
if [ -x /usr/local/bin/trimmy ]; then
    TRIMMY="/usr/local/bin/trimmy"
elif [ -x /opt/homebrew/bin/trimmy ]; then
    TRIMMY="/opt/homebrew/bin/trimmy"
else
    echo "Trimmy CLI not found. Open Trimmy Settings → General → Install CLI Tool"
    exit 1
fi

# Get aggressiveness from argument, default to normal
ARG="${1:-normal}"
case "$ARG" in
    high|h) AGGRESSIVENESS="high" ;;
    low|l) AGGRESSIVENESS="low" ;;
    *) AGGRESSIVENESS="normal" ;;
esac

# Trim clipboard content
pbpaste | "$TRIMMY" --trim - --aggressiveness "$AGGRESSIVENESS"</string>
